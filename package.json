  "dependencies": {
    "@types/jest": "^24.0.15",
    "@types/mapbox-gl": "^0.54.3",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-textarea-autosize": "^4.3.4",
    "@types/redux-logger": "^3.0.7",
    "autoprefixer": "^9.6.1",
    "axios": "^0.19.0",
    "chokidar-cli": "^1.2.2",
    "mapbox-gl": "^1.3.0",
    "moment": "^2.24.0",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^3.7.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@types/prop-types": "^15.7.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2"
  },
  "scripts": {
    "build-task:scss-compile": "node-sass-chokidar --source-map true src/scss/ -o src/dist/css",
    "build-task:autoprefixer": "postcss src/dist/css/*.css --use autoprefixer -d src/dist/css",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar \"src/scss/**/*.scss\" -c \"npm run sass:build\"",
    "dev": "npm-run-all -p sass:* start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
